                <li>{{#link-to "new-platform-flexberry-g-i-s-map-object-setting-l"}}{{t "forms.application.sitemap.map-object-setting.caption"}}{{/link-to}}</li>
              </ul>
            </div>
          </div>
        </div>
      </div>
      <div class="eight wide middle computer four wide large screen four wide widescreen column need-pd-computer">
        <div class="ui card">
          <div class="content">
            <i class="icon-guideline-date card-icon icon" ></i>
            <div class="header">{{t "forms.application.sitemap.components-examples.caption"}}</div>
            <div class="description">
              <ul>
                <li>{{#link-to "components-examples/flexberry-boundingbox/settings-example"}}{{t "forms.application.sitemap.components-examples.flexberry-boundingbox.caption"}}{{/link-to}}</li>
