<div class="flexberry-container">
  <div class="ui stackable doubling grid">
    <div class="row"><h2>{{t "forms.index.greeting"}}</h2></div>
    <div class="row">
      <div class="eight wide middle computer four wide large screen four wide widescreen column need-pd-computer">
        <div class="ui card">
          <div class="content">
            <i class="icon-guideline-earth card-icon icon" ></i>
            <div class="header">{{t "forms.application.sitemap.gis.caption"}}</div>
            <div class="description">
              <ul>
                <li>{{#link-to "maps"}}{{t "forms.application.sitemap.gis.maps.caption"}}{{/link-to}}</li>
