{{map-commands/base
  class=(concat flexberryClassNames.wrapper " " class)
  name="go-to"
  caption=caption
  tooltip=tooltip
  iconClass=iconClass
  leafletMap=leafletMap
  click=(action "onMapCommandButtonClick")
}}

{{#if _gotoDialogHasBeenRequested}}
  {{component "map-commands-dialogs/go-to"
    class=flexberryClassNames.gotoDialog
    visible=_gotoDialogIsVisible
    _availableModes=availableCRS
    leafletMap=leafletMap
    approve=(action "onGoToDialogApprove")
  }}
{{/if}}