<div class="ui centered grid">
  <div class="six wide column">
    <div class="field">
      <label>
        {{t "components.charts.index-chart.typr-charts.caption"}}
      </label>
      {{flexberry-dropdown
        items=_availableTypes
        value=_selectedModeType
      }}
    </div>
    <div class="field">
      <label>
        {{t "components.charts.index-chart.caption-charts"}}
      </label>
      {{flexberry-textbox
        value=_captionChart
      }}
    </div>
    <div class="ui grid">
      <div class="column">
        {{#if _selectedModeType}}
          {{component (concat "charts/type-charts/" _selectedModeType)
            _isObject = _isObjProperties
            _titleChart = _captionChart
            _chartType = _selectedModeType
            onInit=(action "initJsonCharts")
          }}
        {{/if}}
      </div>
    </div>
    <div class="ui grid">
      <div class="right aligned column">
        {{flexberry-button
          caption="Сформировать диаграмму"
          click=(action "onGenerateChart")
        }}
      </div>
    </div>
  </div>
  <div class="nine wide column">
    <div class="containerCR">
    </div>
  </div>
</div>
