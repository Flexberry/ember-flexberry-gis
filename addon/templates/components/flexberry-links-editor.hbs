<div class="field">
    <label>
        {{mosCaption}}
    </label>
    {{flexberry-lookup
        placeholder=placeholder
        readonly=readonly
        value=model.mapObjectSetting
        projection="MapObjectSettingL"
        displayAttributeName="typeName"
        title="Map object setting"
        relatedModel=model
        relationName="mapObjectSetting"
        choose="showLookupDialog"
        remove="removeLookupValue"
        autocomplete=true
        dropdown=false
    }}
</div>