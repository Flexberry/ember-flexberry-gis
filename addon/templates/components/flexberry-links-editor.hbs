<div class="ui grid">
    <div class="fourteen wide column">
        <div class="field">
            <label>
                {{mosCaption}}
            </label>
            {{flexberry-lookup
                placeholder="Нет значения"
                readonly=false
                value=model.mapObjectSetting
                projection="MapObjectSettingL"
                displayAttributeName="typeName"
                title="Map object setting"
                relatedModel=model
                relationName="mapObjectSetting"
                choose="showLookupDialog"
                remove="removeLookupValue"
                autocomplete=true
                dropdown=false
            }}
        </div>
    </div>
    <div class="two wide column">
        <div class="field">
            <label>
                {{allowShowCaption}}
            </label>
            {{flexberry-ddau-checkbox
                class=(concat flexberryClassNames.visibilityCheckbox " " flexberryClassNames.preventExpandCollapse)
                value=model.allowShow
                change=(action "onVisibilityCheckboxChange")
            }}
        </div>
    </div>
</div>