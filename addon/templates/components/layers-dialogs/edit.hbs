{{#flexberry-dialog class=(concat class " " flexberryClassNames.prefix)
  caption=caption
  approveButtonCaption=approveButtonCaption
  denyButtonCaption=denyButtonCaption
  visible=visible
  typeIsReadonly=typeIsReadonly
  approve=(action "onApprove")
  deny=(action "onDeny")
  beforeShow=(action "onBeforeShow")
  beforeHide=(action "onBeforeHide")
  show=(action "onShow")
  hide=(action "onHide")
}}

{{flexberry-edit-layermap
    visible=visible
    cswConnections=cswConnections
    layer=layer
    leafletMap=leafletMap
    _typeIsReadonly=typeIsReadonly
    layerProperties=(action "onLayerProperties")
}}

{{/flexberry-dialog}}
