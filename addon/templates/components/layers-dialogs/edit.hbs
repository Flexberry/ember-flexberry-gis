{{#flexberry-dialog class=(concat class " " flexberryClassNames.prefix)
  caption=caption
  approveButtonCaption=approveButtonCaption
  denyButtonCaption=denyButtonCaption
  visible=visible
  typeIsReadonly=typeIsReadonly
  approve=(action "onApprove")
  deny=(action "onDeny")
  beforeShow=(action "onBeforeShow")
  beforeHide=(action "onBeforeHide")
  show=(action "onShow")
  hide=(action "onHide")
}}
  {{#flexberry-dialog class='rastrMessage'
    caption=captionRastr
    approveButtonCaption=approveButtonCaption
    denyButtonCaption=denyButtonCaption
    visible=rastrMessageVisible
    typeIsReadonly=typeIsReadonly
    approve=(action "onApproveError")
    deny=(action "onDeny")
    beforeShow=(action "onBeforeShow")
    beforeHide=(action "onBeforeHide")
    show=(action "onShow")
    hide=(action "onHide")
  }}
    {{ui-message
      type=typeMessage
      message=message
    }}
  {{/flexberry-dialog}}
  {{flexberry-edit-layermap
    visible=visible
    layer=layer
    leafletMap=leafletMap
    _typeIsReadonly=typeIsReadonly
    links=(get this linksPropertyPath)
    linksPropertyPath=linksPropertyPath
    linksModelName=linksModelName
    parametersModelName=linksParametersModelName
    parametersModelProjection=linksParametersModelProjection
    onInit=(action "initLayerProperties")
    addLayerLink=(action "addLayerLink")
    removeLayerLink=(action "removeLayerLink")
    allowShowLayerLinkCheckboxChange=(action "onLayerLinkChangeVisibility")
    onUploadFile = (action "onUploadFile")
  }}
{{/flexberry-dialog}}
