{{flexberry-checkbox
  value=_canBeSearchedCheckBox
  label=('t' _canBeSearchedCheckBoxLabel)
  readonly=readonly
  class=checkboxClass
}}
<br>
{{flexberry-checkbox
  value=_canBeContextSearchedCheckBox
  label=('t' _canBeContextSearchedCheckBoxLabel)
  readonly=readonly
  class=checkboxClass
}}
<br>
{{flexberry-multiple-select
  selectedItems=value.searchFields
  heading=_searchFieldsSelectorLabel
  allowAdditions=true
  onChange=(action "test")
}}
<br>
{{flexberry-multiple-select
  selectedItems=value.contextSearchFields
  heading=_contextSearchFieldsSelectorLabel
}}
